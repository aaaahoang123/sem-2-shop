    // Hot New Arrivals
    .new_arrivals
        .container
            .row
                .col
                    .tabbed_container
                        .tabs.clearfix.tabs-right
                            .new_arrivals_title Hot New Arrivals
                            ul.clearfix
                                li.active #{top_categories[0].name}
                                li #{top_categories[1].name}
                                li #{top_categories[2].name}
                            .tabs_line
                                span
                        .row
                            .col-lg-12
                                - if (top_categories)
                                    - for (var i = 0; i < top_categories.length; i ++)
                                        .product_panel.panel(class=(i===0?"active":""))
                                            .arrivals_slider.slider
                                                -for (var j = 0; j < top_categories[i].products.length; j ++)
                                                    .arrivals_slider_item
                                                        .border_active
                                                        .product_item.is_new.d-flex.flex-column.align-items-center.justify-content-center.text-center
                                                            .product_image.d-flex.flex-column.align-items-center.justify-content-center
                                                                a(href='/product/' + top_categories[i].products[j].code)
                                                                    img(src=top_categories[i].products[j].images[0] alt=top_categories[i].products[j].name)
                                                            .product_content
                                                                .product_price(style='color: red')
                                                                    | #{"$" + top_categories[i].products[j].price}
                                                                .product_name
                                                                    div
                                                                        a(href='/product/' + top_categories[i].products[j].code title=top_categories[i].products[j].name): p.mx-auto.char-15 #{top_categories[i].products[j].name}
                                                                .product_extras
                                                                    button.product_cart_button.btn-add-to-cart(data-code=top_categories[i].products[j].code onclick='addToCart(this)') Add to Cart
                                            .arrivals_slider_dots_cover

    // Best Sellers
    .best_sellers
        .container
            .row
                .col
                    .tabbed_container
                        .tabs.clearfix.tabs-right
                            .new_arrivals_title Hot Best Sellers
                            ul.clearfix
                                li(hidden='') Top 20
                                li(hidden='') Audio &amp; Video
                                li(hidden='') Laptops &amp; Computers
                            .tabs_line
                                span
                        .bestsellers_panel.panel.active
                            - if (bProducts)
                                // Best Sellers Slider
                                .bestsellers_slider.slider
                                    // Best Sellers Item
                                    - for (let bp of bProducts)
                                        .bestsellers_item.discount
                                            .bestsellers_item_container.d-flex.flex-row.align-items-center.justify-content-start
                                                a(href='/product/' + bp.products[0].code)
                                                    .bestsellers_image
                                                        img(src=bp.products[0].images[0] alt='')
                                                    .bestsellers_content
                                                        .bestsellers_name
                                                            a(href='/product/' + bp.products[0].code) #{bp.products[0].name}
                                                        .bestsellers-price
                                                            h1
                                                            span(style='color: red') $ #{bp.products[0].price}
                                            .bestsellers_fav.active
                                                i.fas.fa-heart
