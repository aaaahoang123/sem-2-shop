extends ../layout/master

block title
    title Home page

block append link
    link(rel='stylesheet' type='text/css' href='/client/plugins/slick-1.8.0/slick.css')
    link(rel='stylesheet' type='text/css' href='/client/styles/main_styles.css')
    link(rel='stylesheet' type='text/css' href='/client/styles/responsive.css')
    style.
        .product_image > a > img {
            max-width: 150px !important;
        }
        .char-15 {
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
            max-width: 15ch;
        }

block content
    //banner
    .banner_2
        .banner_2_background(style='background-image:url(client/images/banner_2_background.jpg)')
        .banner_2_container
            .banner_2_dots
            // Banner 2 Slider
            .owl-carousel.owl-theme.banner_2_slider
                - if (sProducts)
                    - for (let sp of sProducts)
                        .owl-item
                            .banner_2_item
                                .container.fill_height
                                    .row.fill_height
                                        .col-lg-5.col-md-6.fill_height
                                            .banner_2_content
                                                .banner_2_category #{sp.categories[0].name}
                                                .banner_2_title #{sp.name}
                                                .banner_2_text
                                                    h2(style='text-decoration: underline') $ #{sp.price}
                                                .button.banner_2_button
                                                    a(href='/product/'+sp.code) Shop now
                                        .col-lg-7.col-md-6.fill_height
                                            .banner_2_image_container
                                                .banner_2_image
                                                    a(href='/product/'+sp.code)
                                                        img(src=sp.images[0] alt='')
    // Characteristics
    .characteristics
        .container
            .row
                // Char. Item
                .col-lg-3.col-md-6.char_col
                    .char_item.d-flex.flex-row.align-items-center.justify-content-start
                        .char_icon
                            img(src='/client/images/char_1.png' alt='')
                        .char_content
                            .char_title Free Delivery
                            .char_subtitle from $50
                // Char. Item
                .col-lg-3.col-md-6.char_col
                    .char_item.d-flex.flex-row.align-items-center.justify-content-start
                        .char_icon
                            img(src='/client/images/char_2.png' alt='')
                        .char_content
                            .char_title Free Delivery
                            .char_subtitle from $50
                // Char. Item
                .col-lg-3.col-md-6.char_col
                    .char_item.d-flex.flex-row.align-items-center.justify-content-start
                        .char_icon
                            img(src='/client/images/char_3.png' alt='')
                        .char_content
                            .char_title Free Delivery
                            .char_subtitle from $50
                // Char. Item
                .col-lg-3.col-md-6.char_col
                    .char_item.d-flex.flex-row.align-items-center.justify-content-start
                        .char_icon
                            img(src='/client/images/char_4.png' alt='')
                        .char_content
                            .char_title Free Delivery
                            .char_subtitle from $50
