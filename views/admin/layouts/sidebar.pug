section
    // Left Sidebar
    aside#leftsidebar.sidebar
        // User Info
        .user-info
            .image
                img(src='/admin/images/user.png' width='48' height='48' alt='User')
            .info-container
                .name(data-toggle='dropdown' aria-haspopup='true' aria-expanded='false') John Doe
                .email john.doe@example.com
                .btn-group.user-helper-dropdown
                    i.material-icons(data-toggle='dropdown' aria-haspopup='true' aria-expanded='true') keyboard_arrow_down
                    ul.dropdown-menu.pull-right
                        li
                            a(href='javascript:void(0);')
                                i.material-icons person
                                | Profile
                        li.divider(role='seperator')
                        li
                            a(href='javascript:void(0);')
                                i.material-icons group
                                | Followers
                        li
                            a(href='javascript:void(0);')
                                i.material-icons shopping_cart
                                | Sales
                        li
                            a(href='javascript:void(0);')
                                i.material-icons favorite
                                | Likes
                        li.divider(role='seperator')
                        li
                            a(href='javascript:void(0);')
                                i.material-icons input
                                | Sign Out
        // #User Info
        // Menu
        .menu
            ul.list
                li.header MAIN NAVIGATION
                li.sidebar-item.active(data-active="#home")
                    a.menu-toggle(href='#')
                        i.material-icons home
                        span Home
                li.sidebar-item(data-active="#web-config")
                    a.menu-toggle(href='javascript:void(0);')
                        i.material-icons web
                        span Website Config
                    ul.ml-menu
                        li.sidebar-item(data-active="information")
                            a(href='#web-config/information') Information

                        li.sidebar-item(data-active="contact")
                            a(href='#web-config/contact') Contact

                        li.sidebar-item(data-active="top-category")
                            a(href='#web-config/top-category') Top-Category

                li.sidebar-item(data-active="#product-manager")
                    a.menu-toggle(href='javascript:void(0);')
                        i.material-icons shop
                        span Product Manager
                    ul.ml-menu
                        li.sidebar-item(data-active="categories")
                            a(href='#product-manager/categories') Categories
                        li.sidebar-item(data-active="add-category")
                            a(href='#product-manager/add-category') Add Category
                        li.sidebar-item(data-active="brands")
                            a(href='#product-manager/brands') Brands
                        li.sidebar-item(data-active="add-brand")
                            a(href='#product-manager/add-brand') Add Brand
                        li.sidebar-item(data-active="products")
                            a(href='#product-manager/products') Products
                        li.sidebar-item(data-active="add-product")
                            a(href='#product-manager/add-product') Add Product


                li.sidebar-item(data-active="#customer-manager")
                    a.menu-toggle(href='javascript:void(0);')
                        i.material-icons account_circle
                        span Customer Manager
                    ul.ml-menu
                        li.sidebar-item(data-active="customers")
                            a(href='#customer-manager/customers') Customers
                        li.sidebar-item(data-active="add-customer")
                            a(href='#customer-manager/add-customer') Add Customer


                li.sidebar-item(data-active="#warehouse-manager")
                    a.menu-toggle(href='javascript:void(0);')
                        i.material-icons store
                        span Warehouse Manager
                    ul.ml-menu
                        li.sidebar-item(data-active="check")
                            a(href='#warehouse-manager/check') Check
                        li.sidebar-item(data-active="edit")
                            a(href='#warehouse-manager/edit') Edit
                        li.sidebar-item(data-active="logs")
                            a(href='#warehouse-manager/logs') Logs

                li.header LABELS
                li
                    a(href='javascript:void(0);')
                        i.material-icons.col-red donut_large
                        span Important
                li
                    a(href='javascript:void(0);')
                        i.material-icons.col-amber donut_large
                        span Warning
                li
                    a(href='javascript:void(0);')
                        i.material-icons.col-light-blue donut_large
                        span Information
        // #Menu
        // Footer
        .legal
            .copyright
                | &copy; 2016 - 2017
                a(href='javascript:void(0);') AdminBSB - Material Design
                | .
            .version
                b Version:
                |  1.0.5
        // #Footer
    // #END# Left Sidebar
    // Right Sidebar
    aside#rightsidebar.right-sidebar
        ul.nav.nav-tabs.tab-nav-right(role='tablist')
            li.active(role='presentation')
                a(href='#skins' data-toggle='tab') SKINS
            li(role='presentation')
                a(href='#settings' data-toggle='tab') SETTINGS
        .tab-content
            #skins.tab-pane.fade.in.active.in.active(role='tabpanel')
                ul.demo-choose-skin
                    li.active(data-theme='red')
                        .red
                        span Red
                    li(data-theme='pink')
                        .pink
                        span Pink
                    li(data-theme='purple')
                        .purple
                        span Purple
                    li(data-theme='deep-purple')
                        .deep-purple
                        span Deep Purple
                    li(data-theme='indigo')
                        .indigo
                        span Indigo
                    li(data-theme='blue')
                        .blue
                        span Blue
                    li(data-theme='light-blue')
                        .light-blue
                        span Light Blue
                    li(data-theme='cyan')
                        .cyan
                        span Cyan
                    li(data-theme='teal')
                        .teal
                        span Teal
                    li(data-theme='green')
                        .green
                        span Green
                    li(data-theme='light-green')
                        .light-green
                        span Light Green
                    li(data-theme='lime')
                        .lime
                        span Lime
                    li(data-theme='yellow')
                        .yellow
                        span Yellow
                    li(data-theme='amber')
                        .amber
                        span Amber
                    li(data-theme='orange')
                        .orange
                        span Orange
                    li(data-theme='deep-orange')
                        .deep-orange
                        span Deep Orange
                    li(data-theme='brown')
                        .brown
                        span Brown
                    li(data-theme='grey')
                        .grey
                        span Grey
                    li(data-theme='blue-grey')
                        .blue-grey
                        span Blue Grey
                    li(data-theme='black')
                        .black
                        span Black
            #settings.tab-pane.fade(role='tabpanel')
                .demo-settings
                    p GENERAL SETTINGS
                    ul.setting-list
                        li
                            span Report Panel Usage
                            .switch
                                label
                                    input(type='checkbox' checked='')
                                    span.lever
                        li
                            span Email Redirect
                            .switch
                                label
                                    input(type='checkbox')
                                    span.lever
                    p SYSTEM SETTINGS
                    ul.setting-list
                        li
                            span Notifications
                            .switch
                                label
                                    input(type='checkbox' checked='')
                                    span.lever
                        li
                            span Auto Updates
                            .switch
                                label
                                    input(type='checkbox' checked='')
                                    span.lever
                    p ACCOUNT SETTINGS
                    ul.setting-list
                        li
                            span Offline
                            .switch
                                label
                                    input(type='checkbox')
                                    span.lever
                        li
                            span Location Permission
                            .switch
                                label
                                    input(type='checkbox' checked='')
                                    span.lever
    // #END# Right Sidebar
